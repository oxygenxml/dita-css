<?xml version="1.0" encoding="UTF-8"?>
<bookmap xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
    xmlns:opentopic-index="http://www.idiominc.com/opentopic/index"
    class="- map/map bookmap/bookmap " collection-type="sequence">
    <opentopic:map xmlns:opentopic="http://www.idiominc.com/opentopic"
        xmlns:ot-placeholder="http://suite-sol.com/namespaces/ot-placeholder">
        <topicref chunk="select-branch to-content by-content"
            class="- map/topicref bookmap/chapter " collection-type="unordered" href="#unique_2"
            id="unique_2" type="topic">
            <topicmeta class="- map/topicmeta ">
                <navtitle class="- topic/navtitle ">Introduction</navtitle>
            </topicmeta>
        </topicref>
        <!-- Should generate here a link to the index.groups element -->        
    </opentopic:map>
    <topic class="- topic/topic " id="unique_2">
        <title class="- topic/title ">Introduction</title>
    </topic>
    
    <!-- Should generate an ID on this element, linked from the above link. -->
    <opentopic-index:index.groups>
        <opentopic-index:index.group>
            <opentopic-index:label>A</opentopic-index:label>
            <opentopic-index:index.entry value="Archives">
                <opentopic-index:formatted-value>Archives</opentopic-index:formatted-value>
                <opentopic-index:refID value="Archives:"/>
                <opentopic-index:index.entry value="browse">
                    <opentopic-index:formatted-value>browse</opentopic-index:formatted-value>
                    <opentopic-index:refID value="Archives:browse:"/>
                </opentopic-index:index.entry>              
            </opentopic-index:index.entry>
        </opentopic-index:index.group>
    </opentopic-index:index.groups>
</bookmap>
