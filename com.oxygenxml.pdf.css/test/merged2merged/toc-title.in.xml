<?xml version="1.0" encoding="UTF-8"?>
<bookmap xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
    xmlns:opentopic-index="http://www.idiominc.com/opentopic/index"
    class="- map/map bookmap/bookmap " collection-type="sequence">
    <opentopic:map xmlns:opentopic="http://www.idiominc.com/opentopic"
        xmlns:ot-placeholder="http://suite-sol.com/namespaces/ot-placeholder">
        <booktitle class="- topic/title bookmap/booktitle ">
            <mainbooktitle class="- topic/ph bookmap/mainbooktitle ">
                <ph class="- topic/ph " keyref="product" product="author">Oxygen XML Author</ph>
            </mainbooktitle>
        </booktitle>
        <frontmatter class="- map/topicref bookmap/frontmatter ">
            <booklists class="- map/topicref bookmap/booklists ">
                <!-- The navtitle should generate a title element that can be styled later. -->
                <toc class="- map/topicref bookmap/toc " navtitle="This is the TOC."/>
            </booklists>
        </frontmatter>

        <chapter class="- map/topicref bookmap/chapter " href="#unique_5" id="unique_5" locktitle="yes"
            navtitle="Installation Options" type="concept">
            <topicmeta class="- map/topicmeta ">
                <linktext class="- map/linktext ">Installation Options for Oxygen XML
                    Author</linktext>
            </topicmeta>
        </chapter>

        <!-- The navtitle attribute should be used as the link text in TOC. It should also migrate to the topic title, for PDF bookmarks. -->
        <topicref class="- map/topicref " href="#unique_6" id="unique_6" locktitle="yes"
            navtitle="Getting Started Nav Title Attribute" type="concept">
            <topicmeta class="- map/topicmeta ">
                <linktext class="- map/linktext ">Some other value.</linktext>
            </topicmeta>
        </topicref>

        <!-- The navtitle element should be used as the link text. It should also migrate to the topic title. -->
        <topicref class="- map/topicref " href="#unique_7" id="unique_7" locktitle="yes" type="concept">
            <topicmeta class="- map/topicmeta ">
                <navtitle class="- topic/navtitle ">Getting Started Nav Title Element</navtitle>
                <linktext class="- map/linktext ">Some other value.</linktext>
            </topicmeta>
        </topicref>

        <!-- This navtitle should not migrate to the topic title, since it is not marked as locktitle. -->
        <topicref class="- map/topicref " href="#unique_8" id="unique_8"  type="concept">
            <topicmeta class="- map/topicmeta ">
                <navtitle class="- topic/navtitle ">Getting Started Nav Title Element</navtitle>
                <linktext class="- map/linktext ">Some other value.</linktext>
            </topicmeta>
        </topicref>

    </opentopic:map>
    
    <topic class="- topic/topic " id="unique_6" >
      <title class="- topic/title ">Getting Started</title>
    </topic>

    <topic class="- topic/topic " id="unique_7" >
      <title class="- topic/title ">Getting Started</title>
    </topic>

    <topic class="- topic/topic " id="unique_8" >
      <title class="- topic/title ">Getting Started</title>
    </topic>
    
</bookmap>
