<?xml version="1.0" encoding="UTF-8"?>
<bookmap collection-type="sequence" class="- map/map bookmap/bookmap ">
    <opentopic:map xmlns:opentopic="http://www.idiominc.com/opentopic">

        <frontmatter class="- map/topicref bookmap/frontmatter " collection-type="sequence">
            <!-- The frontmatter referred topics must be copied before the opentopic:map (which becomes a TOC)
             and must be filtered from the content. -->
            <topicref class="- map/topicref " href="#fm1">            
            		<topicmeta class="- map/topicmeta " resourceid="trademarks">
                        <navtitle class="- topic/navtitle ">Trademarks</navtitle>
                     
                         <shortdesc class="- map/shortdesc ">The following terms are trademarks of
                            the Retro Tools in the United States, other countries, or
                            both:</shortdesc>
                    </topicmeta>                           
            </topicref>
            <topicref class="- map/topicref " href="#fm2"/>
            <topicref class="- map/topicref " href="#fm3"/>
            <topicref class="- map/topicref " href="#fm4"/>
        </frontmatter>


        <part class="- map/topicref bookmap/part " id="p1" navtitle="A Part">
            
            <topicref class="- map/topicref " href="#remaining1"/>
            <topicref class="- map/topicref " href="#remaining2"/>

        </part>

        <backmatter class="- map/topicref bookmap/backmatter ">
            <!-- The backmatter referred topics must be copied at the end of the content filtered from the content. -->
            <topicref class="- map/topicref " href="#bm1"/>
            <topicref class="- map/topicref " href="#bm2"/>
            <topicref class="- map/topicref " href="#bm3"/>
            <topicref class="- map/topicref " href="#bm4"/>
        </backmatter>
    </opentopic:map>
    <!--  CONTENT  -->    
    <!-- This should be moved in the frontmatter -->
    <topic class="- topic/topic " id="fm1">
        <title class="- topic/title">FM1</title>
    </topic>
    <topic class="- topic/topic " id="fm2">
        <title class="- topic/title">FM2</title>
        <topic class="- topic/topic " id="fm3">
            <title class="- topic/title">FM3</title>
        </topic>
    </topic>
    <topic class="- topic/topic " id="fm4">
        <title class="- topic/title">FM4</title>
    </topic>
    <!-- This should remain in the content. -->
    <topic class="- topic/topic " id="remaining1">
        <title class="- topic/title">REMAINING 1</title>
    </topic>
    <!-- This should be moved in the backmatter -->
    <topic class="- topic/topic " id="bm1">
        <title class="- topic/title">BM1</title>
    </topic>
    <topic class="- topic/topic " id="bm2">
        <title class="- topic/title">BM2</title>
        <topic class="- topic/topic " id="bm3">
            <title class="- topic/title">BM3</title>
        </topic>
    </topic>
    <topic class="- topic/topic " id="bm4">
        <title class="- topic/title">BM4</title>
    </topic>
    <!-- This should remain in the content. -->
    <topic class="- topic/topic " id="remaining2">
        <title class="- topic/title">REMAINING 2</title>
    </topic>

</bookmap>
